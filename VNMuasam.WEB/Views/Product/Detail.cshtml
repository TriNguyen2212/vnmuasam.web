            if (window.pageYOffset > $("#wrapper").position().top) {
                $(".fixed-cart-wrap").addClass("is-show");
            }
            else {
                $(".fixed-cart-wrap").removeClass("is-show");
            }
        };
        $(function () {
            Detail.ShowStickProductCart();
            $(window).on("scroll", function () {
                Detail.ShowStickProductCart();
            });
        });
    </script>
}